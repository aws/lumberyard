{
    "name": [
        "gmock",
        "gmock_main"
    ],
    "source": "@3P:GoogleMock@",
    "description": "Google Mock",
    "includes": [
        "googlemock/include",
        "googletest/include"
    ],
    "defines": [
        "GTEST_HAS_EXCEPTIONS=0",
        "_SILENCE_TR1_NAMESPACE_DEPRECATION_WARNING"
    ],
    "lib_required": "True",
    "platform": {
        "win_x64_vs2013": {
            "libpath_debug": [
                "lib/x64/vc120-md/LIB/Debug"
            ],
            "libpath_release": [
                "lib/x64/vc120-md/LIB/RelWithDebInfo"
            ],
            "gmock/lib": [
                "gmock.lib"
            ],
            "gmock_main/lib": [
                "gmock_main.lib"
            ]
        },
        "win_x64_vs2015": {
            "libpath_debug": [
                "lib/x64/vc140-md/LIB/Debug"
            ],
            "libpath_release": [
                "lib/x64/vc140-md/LIB/RelWithDebInfo"
            ],
            "gmock/lib": [
                "gmock.lib"
            ],
            "gmock_main/lib": [
                "gmock_main.lib"
            ],
            "linkflags" : [
                "/ignore:4099"
            ]
        },
        "win_x64_vs2017": "@win_x64_vs2015",
        "darwin_x64": {
            "libpath_debug": [
                "lib/osx/Debug"
            ],
            "libpath_release": [
                "lib/osx/Release"
