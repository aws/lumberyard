            "libpath_release": [
                "lib/orbis/Release"
            ],
            "gmock/lib": [
                "libgmock.a"
            ]
        },
        "linux_x64": {
            "libpath_debug": [
                "lib/linux/debug"
            ],
            "libpath_release": [
                "lib/linux/release"
