        "android_armv7_clang": {
            "libpath_debug": [
                "lib/android/armv7_clang/Debug"
            ],
            "libpath_release": [
                "lib/android/armv7_clang/Release"
            ],
            "gmock/lib": [
                "libgmock.a"
            ],
            "gmock_main/lib": [
                "libgmock_main.a"
            ]
        },
        "durango_vs2015": {
            "libpath_debug": [
